<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"   android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/botao1"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="30dp"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:text="H"/>
    <Button
        android:id="@+id/botao2"
        android:layout_width="50dp"
        android:layout_marginLeft="5dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/botao1"
        android:text="I"/>
    <Button
        android:id="@+id/botao3"
        android:layout_marginLeft="5dp"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/botao2"
        android:text="A"/>
    <Button
        android:id="@+id/botao4"
        android:layout_marginLeft="5dp"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/botao3"
        android:text="G"/>
    <Button
        android:id="@+id/botao5"
        android:layout_marginLeft="5dp"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/botao4"
        android:text="O"/>
    <Button
        android:id="@+id/botao6"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/botao5"
        android:text="X"/>



</RelativeLayout>
